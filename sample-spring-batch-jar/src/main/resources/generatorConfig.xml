<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry location="lib/db2jcc4.jar" />
	<context id="mydb">
		<jdbcConnection driverClass="com.ibm.db2.jcc.DB2Driver"
			connectionURL="jdbc:db2://localhost:50000/mydb" userId="db2admin"
			password="db2admin" />
		<javaModelGenerator
			targetPackage="jp.co.springbatch.sample.data.entity"
			targetProject="tools/mybatis-generator/" />
		<sqlMapGenerator
			targetPackage="jp.co.springbatch.sample.data.entity"
			targetProject="tools/mybatis-generator/" />
		<javaClientGenerator
			targetPackage="jp.co.springbatch.sample.data.entity"
			targetProject="tools/mybatis-generator/" type="XMLMAPPER" />
		<table schema="MYSCHEMA" tableName="TBS_CUSTOMER" />
	</context>
</generatorConfiguration>
